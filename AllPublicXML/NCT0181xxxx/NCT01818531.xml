  <brief_title>Adductor Canal Block for Medial Compartment Knee Arthroplasty</brief_title>
  <official_title>Evaluation of Ultrasound-Guided Adductor Canal Blockade for Postoperative Analgesia Following Robotic Medial Unicompartmental Knee Replacement</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wake Forest University Health Sciences</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
