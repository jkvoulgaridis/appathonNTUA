  <secondary_outcome>
    <measure>Average Neonatal Weight</measure>
    <time_frame>at the time of discharge (about 2-7 days after birth)</time_frame>
    <description>To evaluate the relationship of change in average neonatal weight to two skill sets of Self-monitoring of blood glucose (SMBG), operational and interpretive, as demonstrated by patient-perceived self-efficacy and adherence to self-care behaviors</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maternal Mean fasting glucose</measure>
    <time_frame>perinatal to postpartum (32 weeks)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maternal Mean post prandial glucose</measure>
    <time_frame>perinatal to postpartum (32 weeks)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Maternal # of Hypoglycemia events (&lt;60 mg/dl)</measure>
    <time_frame>perinatal to postpartum (32 weeks)</time_frame>
  </secondary_outcome>
