  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">34</enrollment>
  <condition>Aging</condition>
  <arm_group>
    <arm_group_label>Hydrogen-rich water</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Hydrogen-rich water (Rejuvenation, HRW Natural Health Products Inc.)
8 ppm of hydrogen
Administered one dose two times per day on an empty stomach in the morning and at the evening</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Control water</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>Tap water
0 ppm of hydrogen
Administered one dose two times per day on an empty stomach in the morning and at the evening</description>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Hydrogen-rich water</intervention_name>
    <description>A tablet that produces hydrogen dissolved in a glass of water</description>
    <arm_group_label>Hydrogen-rich water</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Control Water</intervention_name>
    <description>A glass of tap water</description>
    <arm_group_label>Control water</arm_group_label>
  </intervention>
