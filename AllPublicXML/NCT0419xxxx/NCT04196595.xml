  <primary_outcome>
    <measure>Identify Menstrual Cycle Patterns</measure>
    <time_frame>Up to 10 years</time_frame>
    <description>Characterized using duration of menstrual flow, and cycle length in unit of days.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Epidemiologic Description of Menstrual Cycle</measure>
    <time_frame>Up to 10 years</time_frame>
    <description>Characterized using cycle patterns and self-reported demographics, no unit of measure</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Determine prevalence of gynecologic health conditions</measure>
    <time_frame>Up to 10 years</time_frame>
    <description>Characterized using menstrual cycle patterns and self-reported health conditions, no unit of measure</description>
  </secondary_outcome>
  <enrollment type="Anticipated">500000</enrollment>
