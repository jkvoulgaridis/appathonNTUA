  <primary_outcome>
    <measure>Technical Success</measure>
    <time_frame>2 months to 12 Months</time_frame>
    <description>Stented kidney drains (to bladder) during the planned indwell time with no re-intervention due to obstruction of the stented ureters</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Primary safety endpoint</measure>
    <time_frame>2 months to 12 Months</time_frame>
    <description>Any Serious Adverse Device Effects</description>
  </primary_outcome>
