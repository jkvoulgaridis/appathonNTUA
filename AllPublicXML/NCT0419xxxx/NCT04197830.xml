  <primary_outcome>
    <measure>Survival</measure>
    <time_frame>day 0 to day 5</time_frame>
    <description>death during ICU stay</description>
  </primary_outcome>
  <primary_outcome>
    <measure>hemorrhagic complication</measure>
    <time_frame>day 1</time_frame>
    <description>hemorrhagic complication base on CT scan at day 1</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Neurological evaluation</measure>
