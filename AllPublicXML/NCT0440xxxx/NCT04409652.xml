  <brief_title>Head Position on Preventing Emergence Cough</brief_title>
  <official_title>Department of Anesthesiology and Pain Medicine, Ajou University School of Medicine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ajou University School of Medicine</agency>
      <agency_class>Other</agency_class>
