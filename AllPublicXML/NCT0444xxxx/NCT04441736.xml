  <brief_title>Pos -Extubation High Flow Nasal Cannula</brief_title>
  <official_title>Post Extubation High Flow Nasal Cannula Versus Conventional Oxygen Therapy in Extubation Failure.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cairo University</agency>
      <agency_class>Other</agency_class>
