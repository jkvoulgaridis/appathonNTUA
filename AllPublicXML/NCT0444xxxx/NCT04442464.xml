  <primary_outcome>
    <measure>Intraocular Pressure (IOP)</measure>
    <time_frame>Baseline, every half hour for 4 hours</time_frame>
    <description>Change in intraocular pressure measured in millimeters of mercury (mm Hg)</description>
  </primary_outcome>
