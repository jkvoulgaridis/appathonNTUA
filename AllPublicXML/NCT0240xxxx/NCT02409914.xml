  <brief_title>Brain Metabolism in Polycystic Ovary Syndrome</brief_title>
  <official_title>Brain Metabolism in Women With Polycystic Ovary Syndrome: a PET/MRI Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Université de Sherbrooke</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
