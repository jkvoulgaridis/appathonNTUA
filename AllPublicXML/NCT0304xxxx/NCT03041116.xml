  <start_date type="Actual">July 17, 2017</start_date>
  <completion_date type="Anticipated">July 2022</completion_date>
  <primary_completion_date type="Anticipated">July 2020</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
