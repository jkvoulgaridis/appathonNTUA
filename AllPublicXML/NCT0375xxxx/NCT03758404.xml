  </primary_outcome>
  <secondary_outcome>
    <measure>Improvement in visual function</measure>
    <time_frame>6 Months</time_frame>
    <description>Improvements in visual function as assessed by visual acuity</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Improvement in retinal function</measure>
    <time_frame>6 Months</time_frame>
    <description>Improvements in retinal function as assessed by microperimetry and perimetry</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Improvement in Quality of life</measure>
    <time_frame>6 Months</time_frame>
    <description>Quality of life will be measured by QoL questionnaires</description>
  </secondary_outcome>
