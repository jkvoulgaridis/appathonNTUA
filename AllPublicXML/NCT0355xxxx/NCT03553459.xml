  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      The purpose of the study is to verify the efficacy of using trendelenburg maneuver to predict
      fluid responsiveness in patients with extracorporeal membrane oxygenation
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">July 1, 2018</start_date>
  <completion_date type="Anticipated">December 30, 2022</completion_date>
  <primary_completion_date type="Anticipated">December 30, 2022</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
