  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Involvement of IGF Axis in Ovarian Cancer</measure>
    <time_frame>Two years</time_frame>
    <description>Ovarian cancer specimens taken during surgical procedures will be evaluated and analyzed by flow cytometry assays to determine the involvement of the IGF axis in the immune tumor microenvironment in ovarian cancer</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">40</enrollment>
