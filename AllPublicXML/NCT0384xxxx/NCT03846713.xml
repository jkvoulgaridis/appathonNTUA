  <primary_outcome>
    <measure>Time to intubate</measure>
    <time_frame>Intraoperative (During exchanging single- to double-lumen tube)</time_frame>
    <description>Length of time to exchange endotracheal tube from single- to double-lumen tube using airway exchange catheter.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Postoperative hoarseness (4-points scoring)</measure>
    <time_frame>up to 48 hours after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>postoperative sore throat (4-points scoring)</measure>
    <time_frame>up to 48 hours after surgery</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Vocal cord injuries assessed by videolaryngoscope</measure>
    <time_frame>up to 1 hour after double-lumen tube extubation</time_frame>
    <description>vocal cord injuries observed after tube-exchange</description>
  </secondary_outcome>
