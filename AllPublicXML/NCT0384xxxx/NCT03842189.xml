      <agency>Momenta Pharmaceuticals, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Momenta Pharmaceuticals, Inc.</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
