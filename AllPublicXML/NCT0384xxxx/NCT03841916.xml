  <primary_outcome>
    <measure>the number of women who will have a relation between endometrial thickness and BMI</measure>
    <time_frame>within 2 weeks</time_frame>
    <description>if there is a relation between increased BMI and endometrial thickness</description>
  </primary_outcome>
  <enrollment type="Anticipated">200</enrollment>
