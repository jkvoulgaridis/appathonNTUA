  <brief_title>Validation of Patient Reported Outcome Measures for Use in Hallux Valgus</brief_title>
  <official_title>Validation of the Modified Foot Function Index (mFFI) for Use in Hallux Valgus</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ipsen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Ipsen</source>
