  <primary_outcome>
    <measure>Change in Movement Disorder Society-Unified Parkinson's Disease Rating Scale (MDS-UPDRS) score</measure>
    <time_frame>12 weeks</time_frame>
    <description>Change from baseline (Week 0) in MDS-UPDRS total score</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in Parkinson's Disease Questionnaire-39 (PDQ-39) score</measure>
    <time_frame>12 weeks</time_frame>
    <description>Change from baseline (Week 0) in PDQ-39 summary index score</description>
  </secondary_outcome>
  <secondary_outcome>
