      <agency>QED Therapeutics, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>QED Therapeutics, Inc.</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
