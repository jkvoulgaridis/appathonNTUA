  </study_design_info>
  <primary_outcome>
    <measure>Comparison of the anterior segment parameter results (Pentacam).</measure>
    <time_frame>1 year</time_frame>
    <description>measurement of the central corneal thickness.</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Comparison of the corneal biomechanical parameter results (ocular response analyzer).</measure>
    <time_frame>1 year</time_frame>
    <description>measurement of the corneal hysteresis (CH).</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">38</enrollment>
