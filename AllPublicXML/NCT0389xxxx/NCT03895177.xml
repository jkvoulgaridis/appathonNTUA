  <primary_outcome>
    <measure>accuracy of low kVp CT to assess R0 resection of pancreatic cancer</measure>
    <time_frame>2 weeks after surgery</time_frame>
    <description>Pancreastic cancer resectability</description>
  </primary_outcome>
