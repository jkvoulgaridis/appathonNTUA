      <agency>Trevi Therapeutics</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Trevi Therapeutics</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
