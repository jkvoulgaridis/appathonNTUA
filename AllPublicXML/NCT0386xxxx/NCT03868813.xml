  <primary_outcome>
    <measure>Users' Expectation from a Health Coaching System</measure>
    <time_frame>60 minutes</time_frame>
    <description>This interview with participants help produce knowledge on the users' expectations from a health coaching system</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">20</enrollment>
