    <intervention_name>Trametinib</intervention_name>
    <arm_group_label>Dabrafenib plus Trametinib</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Metastasic melanoma patients treated with Dabrafenib +/- Trametinib in compassionate use.
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients who have received at least one dose of dabrafenib or combination with
             trametinib as part of a compassionate use for the treatment of metastatic melanoma,
             with deadline the start of treatment the April 30, 2014.

        Exclusion Criteria:

          -  Patients with a history not available.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
