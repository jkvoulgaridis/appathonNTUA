  <primary_outcome>
    <measure>analyze acute and late toxicities using CTCAE 4.03</measure>
    <time_frame>Patients will be followed for the duration of their life, an expected average of 20 years</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>analyze quality of life using LASA-3 and FACT-E</measure>
    <time_frame>Patients will be followed for the duration of their life, an expected average of 20 years</time_frame>
  </primary_outcome>
