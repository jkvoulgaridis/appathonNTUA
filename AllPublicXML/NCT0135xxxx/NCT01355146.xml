  </study_design_info>
  <primary_outcome>
    <measure>Patient satisfaction estimated on a 10-ary Likert scale</measure>
    <time_frame>comparison of baseline to 12 months value</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number (per infusion) and severity of infusion-related side effects</measure>
    <time_frame>baseline compared to 12 months</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
