  <primary_outcome>
    <measure>Change of Lung to liver Elastography ratio (LLE ratio) between 24 and 39 weeks of amenorrhea</measure>
    <time_frame>measurements at: 24, 28, 32, 36, 39 weeks of amenorrhea (WA)</time_frame>
    <description>Evolution of the fetal lung elasticity coefficient measured in kilopascals (kPa) compared to the fetal liver elasticity</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">72</enrollment>
