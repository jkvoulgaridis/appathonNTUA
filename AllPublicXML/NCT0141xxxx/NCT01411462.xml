  <primary_outcome>
    <measure>Binary restenosis</measure>
    <time_frame>6 months</time_frame>
    <description>Number of patients with a diameter stenosis &gt;50% assessed by quantitative coronary angiography</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Procedural success</measure>
    <time_frame>1 day</time_frame>
    <description>Number of patients with both post-procedure diameter stenosis &lt;20% assessed by quantitative coronary angiography and TIMI 3 flow</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Major adverse cardiac events</measure>
    <time_frame>6 months</time_frame>
    <description>Number of patients with one of the following events: cardiac death, non-fatal myocardial infarction, target lesion revascularization</description>
  </secondary_outcome>
