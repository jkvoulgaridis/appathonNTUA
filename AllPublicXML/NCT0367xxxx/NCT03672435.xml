  <brief_title>Strabismus Recovery With Proparacaine and Oculocardiac Reflex</brief_title>
  <acronym>OCRprop</acronym>
  <official_title>Does Proparacaine Enhance Strabismus Recovery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alaska Blind Child Discovery</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
