  <brief_title>BlueSync Field Evaluation</brief_title>
  <official_title>BlueSync Field Evaluation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
