  <brief_title>Efficiency and Safety of Water Birth</brief_title>
  <official_title>Efficiency and Safety of Water Birth in Maternity Hospitals in Uncomplicated Pregnancy.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Akusherstvo Pro</agency>
      <agency_class>Other</agency_class>
