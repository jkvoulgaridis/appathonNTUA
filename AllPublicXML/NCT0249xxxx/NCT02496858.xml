  <brief_title>Clinical and Genetic Characteristics of Coronary Artery Disease in Chinese Young Adults</brief_title>
  <acronym>GRAND</acronym>
  <official_title>Clinical and Genetic Characteristics of Coronary Artery Disease in Chinese Young Adults</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shanghai Zhongshan Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
