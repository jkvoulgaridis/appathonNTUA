  <brief_title>Single vs. Multiple Privigen Dose Regimens in Pediatric CIDP</brief_title>
  <official_title>Randomized Study of Single vs. Multiple Privigen Dose Regimens in Pediatric CIDP</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>CSL Behring</agency>
      <agency_class>Industry</agency_class>
