  <start_date type="Actual">June 26, 2019</start_date>
  <completion_date type="Anticipated">March 30, 2022</completion_date>
  <primary_completion_date type="Anticipated">September 29, 2021</primary_completion_date>
  <phase>Phase 4</phase>
