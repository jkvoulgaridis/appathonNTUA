  </study_design_info>
  <primary_outcome>
    <measure>Change in cerebral regional oxygen saturation</measure>
    <time_frame>Before and 3 minutes after normoxic challenges</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">20</enrollment>
  <condition>Hemorrhage</condition>
