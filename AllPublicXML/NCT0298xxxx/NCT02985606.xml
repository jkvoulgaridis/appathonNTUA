  <primary_outcome>
    <measure>Work done in kilojoules (kJ) during a 15 minute time trial on a cycle ergometer</measure>
    <time_frame>15 minutes</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Whole body fuel substrate utilisation measured using indirect calorimetry</measure>
    <time_frame>10 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in plasma caffeine concentration and metabolites</measure>
    <time_frame>2 hours 45 minutes</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in cognitive function measured using stroop test</measure>
    <time_frame>2 hours 45 minutes</time_frame>
  </secondary_outcome>
