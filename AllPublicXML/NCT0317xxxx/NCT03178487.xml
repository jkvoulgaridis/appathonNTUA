  <brief_title>A Study Evaluating the Safety and Efficacy of Upadacitinib in Subjects With Active Ankylosing Spondylitis ( SELECT Axis 1 )</brief_title>
  <acronym>SELECT Axis 1</acronym>
  <official_title>A Multicenter, Randomized, Double-Blind, Placebo-Controlled Study Evaluating the Safety and Efficacy of Upadacitinib in Subjects With Active Ankylosing Spondylitis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AbbVie</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>AbbVie</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
