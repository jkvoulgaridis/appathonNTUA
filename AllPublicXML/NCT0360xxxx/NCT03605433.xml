  <primary_outcome>
    <measure>MACE</measure>
    <time_frame>5 years</time_frame>
    <description>Time from randomization to the first occurrence of either myocardial infarction, stroke, or cardiovascular death</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Graft Patency</measure>
