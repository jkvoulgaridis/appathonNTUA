  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">100</enrollment>
  <condition>Coronary Artery Disease</condition>
  <condition>Saphenous Vein Graft Patency</condition>
  <arm_group>
    <arm_group_label>Oral Anticoagulation+Antiplatelet</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Rivaroxaban 2.5 mg twice daily and Aspirin 100 mg once daily.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Oral Anticoagulation</arm_group_label>
