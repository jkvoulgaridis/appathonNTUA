  <brief_title>Nutrition Insights Day Asia</brief_title>
  <official_title>Nutrition Insights Day Asia to Assess Hospital Malnutrition and the Routine Use of Enteral and Parenteral Nutrition in Patients Following Major Gastrointestinal Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fresenius Kabi</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Iqvia Pty Ltd</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
