  <brief_title>Luminopia One Amblyopia Vision Improvement Study</brief_title>
  <official_title>Luminopia One Amblyopia Vision Improvement Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Luminopia</agency>
