  <brief_title>Behavior Management Techniques Adopted by Pediatric Dentists in Egypt</brief_title>
  <official_title>Behavior Management Techniques Adopted by Pediatric Dentists in Egypt: Cross-sectional Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cairo University</agency>
      <agency_class>Other</agency_class>
