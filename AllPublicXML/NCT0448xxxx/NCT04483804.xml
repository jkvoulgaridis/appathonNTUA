  <brief_title>Application of Radiomics in Breast Cancer</brief_title>
  <official_title>Application of Radiomics in Correlation Between Ultrasound Characteristics and Pathological Prognostic Factors in Breast Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Second Affiliated Hospital, School of Medicine, Zhejiang University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
