  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Density Gradient Centrifugation</intervention_name>
    <description>Semen processing is done by double layer density gradient method.</description>
    <arm_group_label>Density Gradient Centrifugation (DGC)</arm_group_label>
    <other_name>DGC</other_name>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Physiological ICSI</intervention_name>
    <description>Sperm selection by PICSI dishes depending on hyaluronan binding of sperm head</description>
    <arm_group_label>Physiological ICSI (PICSI)</arm_group_label>
    <other_name>PICSI</other_name>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Magnetic activated cell sorting</intervention_name>
