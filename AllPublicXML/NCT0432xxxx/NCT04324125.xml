  <brief_title>Chemotherapy Combined With Immunotherapy in HER 2 Insertion or Amplification Advanced NSCLC</brief_title>
  <official_title>Chemotherapy or Chemotherapy Plus PD-1 Antibody in HER 2 Insertion or Amplification Advanced Non-small Cell Lung Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hunan Province Tumor Hospital</agency>
      <agency_class>Other</agency_class>
