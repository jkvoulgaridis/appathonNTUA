  <brief_title>Endurant Stent Graft Natural Selection Global Postmarket Registry</brief_title>
  <acronym>ENGAGE</acronym>
  <official_title>Endurant Stent Graft Natural Selection Global Postmarket Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Endovascular</agency>
