  <brief_title>Optimize PRO Study</brief_title>
  <official_title>Optimize PRO Transcatheter Aortic Valve Replacement Post Market Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiovascular</agency>
      <agency_class>Industry</agency_class>
