    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change of satiety</measure>
    <time_frame>0 and 20, 40, 60, 90 and 120 minutes post juice consumption</time_frame>
    <description>Change of hunger feeling (appetite) will be examined using visual analog scale (0-10), 0 least and 10 greatest</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change of glucose level</measure>
    <time_frame>0 and 20, 40, 60, 90 and 120 minutes post juice consumption</time_frame>
    <description>Change of blood glucose level will be determined to examine effects of watermelon juice consumption vs red juice</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Change of insulin level</measure>
    <time_frame>0 and 20, 40, 60, 90 and 120 minutes post juice consumption</time_frame>
    <description>Change of blood insulin level will be determined to examine effects of watermelon juice consumption vs red juice</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">80</enrollment>
  <condition>Satiety</condition>
  <arm_group>
    <arm_group_label>Red juice</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Subjects consume 8 fl oz of red juice daily for 8 weeks</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Watermelon juice</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Subjects consume 8 fl oz of watermelon juice daily for 8 weeks</description>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Red juice</intervention_name>
