  <brief_title>Autologous Conditioned Plasma (ACP) Intra-articular (IA) Injections for Knee Osteoarthritis (OA)</brief_title>
  <official_title>Autologous Conditioned Plasma (ACP) Intra-articular (IA) Injections for Knee Osteoarthritis (OA)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arthrex, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Arthrex, Inc.</source>
  <oversight_info>
