  <brief_title>Tx for Child Sexual Behavior Problems</brief_title>
  <official_title>Development and Pilot Testing of a Treatment for Children With Sexual Behavior Problems</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Milton S. Hershey Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
