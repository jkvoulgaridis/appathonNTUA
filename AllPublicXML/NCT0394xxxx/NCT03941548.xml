    <primary_purpose>Treatment</primary_purpose>
    <masking>Triple (Participant, Care Provider, Investigator)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Relative change in Severity of Alopecia Tool (SALT) score for each dose regimen</measure>
    <time_frame>24 Weeks</time_frame>
  </primary_outcome>
  <other_outcome>
    <measure>Number of Participants with Adverse Events as a Measure of Safety</measure>
    <time_frame>24 Weeks</time_frame>
  </other_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">66</enrollment>
