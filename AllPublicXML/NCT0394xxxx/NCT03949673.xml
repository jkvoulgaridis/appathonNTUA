  <completion_date type="Anticipated">June 21, 2021</completion_date>
  <primary_completion_date type="Anticipated">June 21, 2021</primary_completion_date>
  <phase>Phase 2</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>N/A</allocation>
