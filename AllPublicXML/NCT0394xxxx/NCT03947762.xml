  <brief_title>Lanreotide 120mg Effectiveness in Subjects With Pancreatic Neuroendocrine Tumours (PanNET) in Routine Clinical Practice.</brief_title>
  <acronym>PanNET</acronym>
  <official_title>Lanreotide 120 mg Effectiveness in Subjects With Locally Advanced or Metastatic Pancreatic Neuroendocrine Tumours (PanNET) in Routine Clinical Practice</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ipsen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
