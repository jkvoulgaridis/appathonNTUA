  <primary_outcome>
    <measure>Spatial working memory strategy index</measure>
    <time_frame>8 weeks</time_frame>
    <description>cognitive function; scores range from 4 to 28, with lower scores representing better outcome</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Reaction time</measure>
    <time_frame>8 weeks</time_frame>
    <description>cognitive function; milliseconds, with faster times representing better outcome</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Paired Associates Learning</measure>
    <time_frame>8 weeks</time_frame>
    <description>cognitive function; scores range from 0 to 70, with lower scores indicating better outcome</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Multitasking Test</measure>
