  <primary_outcome>
    <measure>Postoperative analgesia</measure>
    <time_frame>24 hours</time_frame>
    <description>Postoperative pain measured by visual analogue scale from 0 to 10 measured over 24h where 0 is better and 10 means worst pain</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Analgesic requirement</measure>
    <time_frame>24 hours</time_frame>
    <description>Investigators measured the total dose of morphine consumption in mg over 24hour</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
