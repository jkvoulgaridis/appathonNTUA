    <intervention_name>spartalizumab (PDR001)</intervention_name>
    <description>100 mg lyophilisate in vial received 400 mg every 4 weeks</description>
    <arm_group_label>spartalizumab (PDR001) + regorafenib</arm_group_label>
    <other_name>PDR001</other_name>
  </intervention>
