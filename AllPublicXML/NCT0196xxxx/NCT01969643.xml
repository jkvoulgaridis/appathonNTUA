    <time_frame>Through 1 month following last dose; up to approximately 2 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Blood concentrations of SGN-LIV1A and metabolites</measure>
    <time_frame>Through 3 weeks after dosing; up to approximately 2 years</time_frame>
  </secondary_outcome>
  <secondary_outcome>
