  <brief_title>Impact of Gamma-OH on Sleep in ICU Patients</brief_title>
  <acronym>GO-SLEEP</acronym>
  <official_title>Impact of Gamma-OH on Sleep in ICU Patients Difficult to Wean From Mechanical Ventilation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Poitiers University Hospital</agency>
      <agency_class>Other</agency_class>
