  <brief_title>Effects of Exercise Training on Fatigue in Thyroid Cancer Survivors</brief_title>
  <official_title>Effects of Exercise Training on Fatigue in Thyroid Cancer Survivors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
