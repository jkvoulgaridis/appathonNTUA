  <brief_title>Collection of Placenta for Mitochondria Isolation</brief_title>
  <official_title>Collection of Placenta for Mitochondria Isolation for Use in Mitochondrial Augmentation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minovia Therapeutics Ltd.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
