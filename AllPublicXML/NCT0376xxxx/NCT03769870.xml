    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Cmax,ss(Maximum concentration at steady state)</measure>
    <time_frame>24 hours</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>AUCτ,ss(Area under the concentration-time curve at steady state)</measure>
    <time_frame>24hours</time_frame>
  </primary_outcome>
