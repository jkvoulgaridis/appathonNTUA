  <brief_title>Comparison of Two Silicone Hydrogel Toric Contact Lenses</brief_title>
  <official_title>Axis Orientation Comparison of Two Silicone Hydrogel Toric Contact Lenses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
