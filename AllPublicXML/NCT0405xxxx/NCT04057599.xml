  <brief_title>Ileus After Lumbar Spine Surgery</brief_title>
  <official_title>Investigation of Postoperative Ileus After Lumbar Spine Surgery: a Prospective Observational Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Taiwan University Hospital</agency>
      <agency_class>Other</agency_class>
