  </study_design_info>
  <primary_outcome>
    <measure>Pharmacokinetics : Cmax</measure>
    <time_frame>0-24 hours</time_frame>
    <description>Maximum plasma concentration</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Pharmacokinetics : AUCt</measure>
    <time_frame>0-24 hours</time_frame>
    <description>Area under the concentration-time curve</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetics : AUCinf</measure>
