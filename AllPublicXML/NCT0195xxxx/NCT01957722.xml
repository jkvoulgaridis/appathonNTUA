  <primary_outcome>
    <measure>Knee injury and Osteoarthritis Outcome Score (KOOS) - Pain Subdomain</measure>
    <time_frame>24 months post treatment</time_frame>
    <description>As measured by a change in KOOS (pain subdomain) score at 24 months from baseline</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Knee injury and Osteoarthritis Outcome Score (KOOS) - Function Subdomain</measure>
    <time_frame>24 months post treatment</time_frame>
    <description>As measured by a change in KOOS score (activity of daily living subdomain) at 24 months from baseline</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">233</enrollment>
