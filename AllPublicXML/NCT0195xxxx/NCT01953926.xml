    <arm_group_label>Neratinib and Paclitaxel</arm_group_label>
    <other_name>Taxol</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Fulvestrant</intervention_name>
