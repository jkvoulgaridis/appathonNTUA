  <brief_title>Ultrasound for Guidewire Detection</brief_title>
  <official_title>Dialysis Catheter Insertion With Substernal Ultrasound Guidance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wroclaw Medical University</agency>
