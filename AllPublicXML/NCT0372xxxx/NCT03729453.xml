  <brief_title>Intra-operative Pancreatoscopy in Patients With IPMN</brief_title>
  <official_title>Intra-operative Pancreatoscopy in Patients With Intraductal Papillary Mucinous Neoplasm (IPMN)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Boston Scientific Corporation</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
