  <brief_title>Golimumab for Adherence in Rheumatoid Arthritis</brief_title>
  <acronym>GO FAR</acronym>
  <official_title>Golimumab for Adherence in Rheumatoid Arthritis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Janssen Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
