  <primary_outcome>
    <measure>Characterize the microbiome [bacteria and fungal community]</measure>
    <time_frame>10 years</time_frame>
    <description>Identifying relative abundance of individual bacterial and fungal members.</description>
  </primary_outcome>
  <enrollment type="Actual">74</enrollment>
