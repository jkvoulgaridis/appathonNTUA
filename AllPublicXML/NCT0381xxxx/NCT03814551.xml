  <brief_summary>
    <textblock>
      This study examines the impact of health signage on beverage consumption in a cafeteria.
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date type="Actual">November 24, 2018</start_date>
  <completion_date type="Actual">May 21, 2019</completion_date>
  <primary_completion_date type="Actual">May 21, 2019</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Change in self-reported sugar-sweetened beverage consumption</measure>
    <time_frame>5 months</time_frame>
    <description>Beverage frequency questionnaire assessing whether various beverages were consumed &quot;4 or more times per day,&quot; &quot;3 times per day,&quot; &quot;2 times per day,&quot; &quot;1 time per day,&quot; &quot;4-6 times per week,&quot; &quot;2-3 times per week,&quot; &quot;one time per week or less,&quot; or &quot;never&quot; in the last/current semester.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in cafeteria sugar-sweetened beverage usage</measure>
    <time_frame>5 months</time_frame>
    <description>Based on inventory and syrup/beverage purchase data and customer counts.</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">911</enrollment>
  <condition>Diet Habit</condition>
  <arm_group>
    <arm_group_label>Comparison</arm_group_label>
    <description>Cohort of participants who regularly eat in cafeteria without health signage.</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Signage</arm_group_label>
    <description>Cohort of participants who regularly eat in cafeteria in which health signage is placed.</description>
  </arm_group>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>Signage</intervention_name>
    <description>Placement of signage discouraging consumption of beverages with added sugar</description>
    <arm_group_label>Signage</arm_group_label>
  </intervention>
