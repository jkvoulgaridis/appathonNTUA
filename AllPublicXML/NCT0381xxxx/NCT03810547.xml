  <brief_title>Abdominoplasty Under Spinal Anesthesia</brief_title>
  <official_title>A Comparative Study Between Spinal and General Anesthesia for Abdominoplasty.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ain Shams University</agency>
      <agency_class>Other</agency_class>
