    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Safety and tolerability of long-term TEZ/IVA treatment based on adverse events (AEs) and serious adverse events (SAEs)</measure>
    <time_frame>from baseline through safety follow-up (up to 28 days after Last Dose at Week 96)</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Absolute change in lung clearance index2.5 (LCI2.5)</measure>
    <time_frame>from baseline through 96 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in sweat chloride</measure>
    <time_frame>from baseline through 96 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in Cystic Fibrosis Questionnaire-Revised (CFQ-R) respiratory domain score</measure>
    <time_frame>from baseline through 96 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in body mass index (BMI)</measure>
    <time_frame>from baseline through 96 weeks</time_frame>
  </secondary_outcome>
