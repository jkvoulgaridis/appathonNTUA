  <brief_title>Chicory Fiber Effect on Satiety and GI Tolerance</brief_title>
  <official_title>The Effects of 2 Inulin Sources on Satiety and Gastrointestinal Tolerance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Minnesota</agency>
      <agency_class>Other</agency_class>
