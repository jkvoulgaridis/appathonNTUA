  <brief_title>Immunotherapy in Combination With Chemoradiation in Patients With Advanced Solid Tumors</brief_title>
  <acronym>CLOVER</acronym>
  <official_title>A Phase I Multicenter Study of Immunotherapy in Combination With Chemoradiation in Patients With Advanced Solid Tumors (CLOVER)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
