  <brief_title>Randomized Evaluation of Ten Allergy Skin Prick Test Devices</brief_title>
  <official_title>Randomized Evaluation of Ten Allergy Skin Prick Test Devices</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Johns Hopkins University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
