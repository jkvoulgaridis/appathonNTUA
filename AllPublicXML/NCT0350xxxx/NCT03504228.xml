    <measure>Death</measure>
    <time_frame>12 months</time_frame>
    <description>Stroke related</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Major Stroke</measure>
    <time_frame>12 months</time_frame>
    <description>Major</description>
