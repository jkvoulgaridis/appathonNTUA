  <primary_outcome>
    <measure>Fistula position</measure>
    <time_frame>Day 1</time_frame>
    <description>Dorsal? Ventral? Lateral - right or left?</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Fistula level</measure>
    <time_frame>Day 1</time_frame>
    <description>Level I-IV (how much of the sphincter is affected?)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Fistula length</measure>
    <time_frame>Day 1</time_frame>
    <description>Shorter or longer than 3 cm?</description>
  </primary_outcome>
