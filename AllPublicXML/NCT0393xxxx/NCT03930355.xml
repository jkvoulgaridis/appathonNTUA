  <brief_title>Perioperative Transfusion Related Outcome</brief_title>
  <official_title>Effect of Perioperative Transfusion on Early and Late Outcome After Major Elective Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Diskapi Teaching and Research Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
