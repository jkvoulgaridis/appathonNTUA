  <brief_title>Progesterone Supplementation in Threatened Abortion</brief_title>
  <acronym>Prothreat</acronym>
  <official_title>Progesterone Supplementation in Threatened Abortion: Is it a Sound Practice?</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tanta University</agency>
      <agency_class>Other</agency_class>
