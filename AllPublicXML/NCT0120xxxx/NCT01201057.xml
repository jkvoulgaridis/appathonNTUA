  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Women aged 18-45 years with a diagnosis of BV according to Amsel's criteria and a
             Nugent score of ≥4

          -  Otherwise healthy

        Exclusion Criteria:

          -  No active STIs
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
