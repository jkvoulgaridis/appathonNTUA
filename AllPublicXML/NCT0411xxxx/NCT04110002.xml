  <brief_title>An Injury Prevention Program for Professional Ballet</brief_title>
  <official_title>An Injury Prevention Program for Professional Ballet: a Randomized Controlled Investigation</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>The Methodist Hospital System</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>The Methodist Hospital System</source>
  <oversight_info>
