  </study_design_info>
  <primary_outcome>
    <measure>Incidence or severity of acute kidney injury (AKI) in the first 48 hours after cardiopulmonary pulmonary bypass (CPB)</measure>
    <time_frame>From start of CPB through 48 hours after CPB</time_frame>
    <description>Kidney Disease Improving Global Outcomes (KDIGO) definitions for AKI</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in maximum plasma free hemoglobin and activated complement from start of CPB through post-surgery day 3</measure>
    <time_frame>From start of CPB through post-surgery day 3</time_frame>
    <description>Blood sample indices of effects from cardiac surgery with CPB</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Severity or duration of AKI in the first 7 days after CPB</measure>
    <time_frame>From start of CPB through 7 days post-cardiac surgery</time_frame>
    <description>KDIGO definitions for AKI</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">400</enrollment>
