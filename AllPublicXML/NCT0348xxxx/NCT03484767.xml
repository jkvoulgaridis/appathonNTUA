  </study_design_info>
  <primary_outcome>
    <measure>Change in plasma methylmalonic acid levels (MMA only)</measure>
    <time_frame>Baseline through 12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Frequency of disease related clinical events in enrolled participants (mut0 and mut- MMA patients)</measure>
    <time_frame>Baseline through 12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Changes in plasma 2-MC levels (PA only)</measure>
    <time_frame>Baseline through 12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Changes in plasma 3-HP levels (PA only)</measure>
    <time_frame>Baseline through 12 months</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>Frequency of disease related clinical events in enrolled participants (PA patients)</measure>
    <time_frame>Baseline through 12 months</time_frame>
  </primary_outcome>
