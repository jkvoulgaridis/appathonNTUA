  <primary_outcome>
    <measure>pain score</measure>
    <time_frame>24 hours</time_frame>
    <description>Visual analogue score from 0 to 10 will be used to assess pain postoperative. 0 means no pain. 10 is worst pain</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>anaesthetic consumption</measure>
    <time_frame>4 hours</time_frame>
    <description>calculating the total anaesthetic MAC consumed</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>diaphragmatic excursion</measure>
    <time_frame>24 hours</time_frame>
    <description>assessment of diaphragmatic excursion preoperative and postoperative using low frequency ultrasound probe</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
