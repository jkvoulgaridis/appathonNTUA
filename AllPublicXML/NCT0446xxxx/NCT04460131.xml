  <brief_title>Soluble ST2 and Cardiovascular Outcome</brief_title>
  <official_title>Association Between Soluble ST2 and Cardiovascular Outcome After Cardiovascular Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nanjing Medical University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
