  <brief_title>Study of Aortic Root Reimplantation Procedure</brief_title>
  <acronym>STAR</acronym>
  <official_title>Superiority Trial of Aortic Root Reimplantation Procedure Versus Aortic Valve Reimplantation Procedure</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Meshalkin Research Institute of Pathology of Circulation</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
