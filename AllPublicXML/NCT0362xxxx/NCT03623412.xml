  <brief_title>Women With Minor Degrees of Glucose Intolerance and the Postpartum Glucose Tolerance Test</brief_title>
  <official_title>Women With Minor Degrees of Glucose Intolerance and the Postpartum Glucose</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Clalit Health Services</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Clalit Health Services</source>
  <oversight_info>
