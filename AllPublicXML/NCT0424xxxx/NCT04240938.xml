  <brief_title>Adult Dacryocystocele Among Patients With Epiphora</brief_title>
  <official_title>Adult Encysted Dacryocystocele: Prevalence Among Patients With Epiphora and Its Management</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minia University</agency>
      <agency_class>Other</agency_class>
