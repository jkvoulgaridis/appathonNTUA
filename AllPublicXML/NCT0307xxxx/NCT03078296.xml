  <brief_title>Burnout Among Members of AAGL</brief_title>
  <official_title>Burnout and Associated Factors Among Members of the American Association of Gynecologic Laparoscopists</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Gaby Moawad</agency>
