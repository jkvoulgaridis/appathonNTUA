  <brief_title>Rotation Medical Bioinductive Implant Database Registry</brief_title>
  <acronym>REBUILD</acronym>
  <official_title>Rotation MEdical BioindUctive ImpLant Database (REBUILD) Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Smith &amp; Nephew, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
