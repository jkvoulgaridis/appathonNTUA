  <brief_title>Real-world Daptomycin Use in Chinese ICUs</brief_title>
  <official_title>Real-world Effectiveness and Safety on Daptomycin in Gram-Positive Infections in Chinese Intensive Care Units</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>West China Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
