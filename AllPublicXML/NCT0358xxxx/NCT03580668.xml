    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Proportion of Participants with HIV-1 RNA Suppression (HIV RNA &lt; 50 copies/mL) at 12 Months after initiating or switching to B/F/TAF</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <secondary_outcome>
