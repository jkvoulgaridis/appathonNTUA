  <brief_title>Trial of Acupuncture for Radiation-Induced Xerostomia in Head and Neck Cancer</brief_title>
  <acronym>ACUPUNCTURE</acronym>
  <official_title>A Phase III Prospective Randomized Trial of Acupuncture for Treatment of Radiation-Induced Xerostomia in Patients With Head and Neck Cancer</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Wake Forest University Health Sciences</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
