  <brief_title>Sleep Timing and Insulin Resistance in Adolescents With Obesity</brief_title>
  <official_title>Investigating the Relationship Between Circadian Phase and Insulin Resistance in Obese Adolescents</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Colorado, Denver</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
