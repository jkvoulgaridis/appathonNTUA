  <brief_title>Effectiveness of Two Water-Based Potassium Oxalate Desensitizers</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Procter and Gamble</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
