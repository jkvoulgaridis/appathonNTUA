  <brief_title>Effect of Passive Ultrasonic Irrigation on the Reduction of Bacteria and Endotoxins in Root Canals</brief_title>
  <official_title>Clinical Evaluation of the Effect of Passive Ultrasonic Irrigation on the Reduction of Bacteria and Endotoxins in Root Canals</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Sao Paulo</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>University of Sao Paulo</source>
