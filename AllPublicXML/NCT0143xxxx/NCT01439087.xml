  <brief_title>Optical Frequency Domain Imaging for Assessing Colonic Polyps</brief_title>
  <official_title>Pilot Study to Evaluate Optical Frequency Domain Imaging as a Tool for Assessing Colonic Polyps</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Massachusetts General Hospital</agency>
      <agency_class>Other</agency_class>
