  <primary_outcome>
    <measure>The safety of multiple doses of COR-001 as measured by the incidence of adverse events</measure>
    <time_frame>35 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetic analysis: maximum serum drug concentrations (Cmax)</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetic analysis: area under the serum drug concentration-time curve (AUC)</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacokinetic analysis: terminal elimination half-life (t1/2)</measure>
    <time_frame>24 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic analysis: change from baseline in hemoglobin</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic analysis: change from baseline in Erythropoietin Resistance Index (ERI)</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic analysis: change from baseline in reticulocyte hemoglobin (CHr)</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic analysis: change from baseline in transferrin saturation (TSAT)</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Pharmacodynamic analysis: change from baseline in serum ferritin</measure>
    <time_frame>12 weeks</time_frame>
  </secondary_outcome>
