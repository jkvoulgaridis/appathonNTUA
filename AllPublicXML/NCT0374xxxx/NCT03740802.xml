  <brief_title>Hypophosphatemia as a Predictor in Surgical Resuscitation Sepsis</brief_title>
  <official_title>Evaluation of Hypophosphatemia as a Predictor of Sepsis in Surgical Resuscitation Units.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Brest</agency>
      <agency_class>Other</agency_class>
