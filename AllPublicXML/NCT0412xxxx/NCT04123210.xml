  <brief_title>Estimating Average Vitamin A Requirements in Indonesian and American Women</brief_title>
  <official_title>Development of 13C-Retinol Isotope Dilution Techniques</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
      <agency_class>Other</agency_class>
