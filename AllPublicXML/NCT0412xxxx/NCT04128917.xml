  </study_design_info>
  <primary_outcome>
    <measure>H. pylori eradication rate</measure>
    <time_frame>Day 49</time_frame>
    <description>Assess H. pylori eradication rate by UBT</description>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">102</enrollment>
