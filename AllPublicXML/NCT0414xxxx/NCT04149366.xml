  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>Retainer</intervention_name>
    <description>Orthodontic retainer</description>
    <arm_group_label>group 1 (Wrapround retainer)</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        patients that have finished fixed orthodontic treatment with no signs or symptoms of
        temporomandibular joint disorders
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  patients finished fixed orthodontic treatment with no signs or symptoms of
             temporomandibular joint disorders

          -  patients indicated for removable retainer

        Exclusion Criteria:

          -  any systemic disease

          -  chronic medication uptake

          -  bad oral hygiene pathological conditions of temporomandibular joint
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>15 Years</minimum_age>
    <maximum_age>35 Years</maximum_age>
  </eligibility>
