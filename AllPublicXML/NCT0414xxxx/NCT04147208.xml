  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Chronic HBV infection population

          -  HBeAg positive

          -  HBsAg≥250 IU/mL

          -  No cirrhosis

        Exclusion Criteria:

          -  AST&gt;5×ULN

          -  Platelet count less than 90E+09/L

          -  TBil＞1.5×ULN

          -  albumin&lt;35 g/L

          -  INR&gt;1.5

          -  AFP&gt;50 ng/mL
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>65 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
