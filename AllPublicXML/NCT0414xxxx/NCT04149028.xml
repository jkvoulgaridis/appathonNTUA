  <primary_outcome>
    <measure>Resumption of ovarian hormonal function</measure>
    <time_frame>3-6 months</time_frame>
    <description>Measurement of Estradiol level on Day 3 of cycle</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Resumption of ovarian folliclugenesis</measure>
    <time_frame>3-6 months</time_frame>
    <description>Assessment of the presence of oocytes by trans vaginal ultrasound on day 3</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Resumption of menstruation</measure>
    <time_frame>3-6 months</time_frame>
    <description>Occurrence of Menstruation after secondary amenrorrhea by questionnaire</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Fertility potential</measure>
    <time_frame>3-6 months</time_frame>
    <description>Potential to get pregnant after ICSI procedure</description>
  </secondary_outcome>
