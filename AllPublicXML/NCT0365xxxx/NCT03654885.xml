  <brief_title>XEN-45 Gel Stent Versus Trabeculectomy in Glaucoma: Gold-Standard Pathway Study (GPS)</brief_title>
  <acronym>GPS</acronym>
  <official_title>XEN45 Gel Stent Versus Trabeculectomy in Open-angle Glaucoma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
