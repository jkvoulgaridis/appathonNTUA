  </study_design_info>
  <primary_outcome>
    <measure>Evaluation of morphine consumption at 24 postoperative hours</measure>
    <time_frame>24 hours</time_frame>
    <description>Morphine consumption (Patient Controlled Analgesia) statement during the first 24 postoperative hours</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Evaluation of Visual Analogue Scale scores at 24 postoperative hours</measure>
    <time_frame>24 hours</time_frame>
    <description>Visual Analogue Scale scores statement during the first 24 postoperative hours (minimum pain 0 - maximum pain 10)</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">50</enrollment>
  <condition>Aortic Aneurysm, Abdominal</condition>
