  <brief_title>Comparison of Chlorpromazine or Pentobarbital Premedications for Pediatric Imaging Procedures</brief_title>
  <acronym>PREIMAPED</acronym>
  <official_title>Comparison of Sedation Using Pentobarbital or Chlorpromazine in Pediatric Non-invasive Imaging Procedure: A Before and After Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University Hospital, Brest</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
