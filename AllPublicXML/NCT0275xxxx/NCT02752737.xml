  <brief_title>Cryo AF Global Registry</brief_title>
  <official_title>Cryo AF Global Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm and Heart Failure</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
