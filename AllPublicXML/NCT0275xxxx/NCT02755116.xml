  <brief_title>Olanzapine for Nausea After Surgery</brief_title>
  <official_title>Olanzapine for the Treatment of Post-Discharge Nausea and Vomiting After Ambulatory Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Jaime B Hyman</agency>
      <agency_class>Other</agency_class>
