  <brief_title>Exercise During Pregnancy and Urinary Incontinence</brief_title>
  <official_title>Effect of Physical Exercise Program During Pregnancy on Urinary Incontinence</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universidad Politecnica de Madrid</agency>
      <agency_class>Other</agency_class>
