  <brief_title>Effect of Semi-sitting Position on Mask Ventilation</brief_title>
  <official_title>Effect of Semi-sitting Position on Mask Ventilation in Anesthetized and Paralyzed Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>SMG-SNU Boramae Medical Center</agency>
      <agency_class>Other</agency_class>
