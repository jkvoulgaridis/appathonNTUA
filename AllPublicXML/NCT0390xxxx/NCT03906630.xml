    <time_perspective>Cross-Sectional</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Agreement with MRI findings as they pertain to presence or absence of a rotator cuff tear</measure>
    <time_frame>Within 1 month of exam</time_frame>
    <description>% of exams that agreed with the MRI findings</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
