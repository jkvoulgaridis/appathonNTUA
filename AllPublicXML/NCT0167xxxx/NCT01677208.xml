    <measure>The Score of effect on symptoms of &quot;Xueyue Zheng&quot;(Blood Stasis syndrome)</measure>
    <time_frame>Day 0, Day7, Day 14, Day 30, Day 60, Day 90</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Barthel Index score ≥90</measure>
    <time_frame>Day 0, Day7, Day 14, Day 30, Day 60, Day 90</time_frame>
  </secondary_outcome>
