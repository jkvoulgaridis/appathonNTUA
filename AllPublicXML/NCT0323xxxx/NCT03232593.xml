  <brief_title>A Study of Atezolizumab (TecentriqÂ®) in Ministry of Food and Drug Safety (MFDS)-Approved Indication(s)</brief_title>
  <official_title>Post-Marketing Surveillance of Tecentriq in MFDS-Approved Indication(s)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hoffmann-La Roche</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
