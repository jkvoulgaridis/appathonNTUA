  <primary_outcome>
    <measure>Complete response rate (CRR)</measure>
    <time_frame>2 years</time_frame>
    <description>CRR based on Lugano classification response criteria</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Overall response rate (ORR)</measure>
    <time_frame>2 years</time_frame>
    <description>ORR, including complete response (CR) and partial response (PR)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Duration of response (DOR)</measure>
