  <brief_title>A Trial Comparing Combined Spinal- Epidural Dosing Strategies for External Cephalic Version</brief_title>
  <official_title>A Randomized Controlled Trial Comparing Combined Spinal- Epidural Dosing Strategies for External Cephalic Version</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
