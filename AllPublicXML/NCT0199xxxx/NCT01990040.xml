  <brief_title>Registry for Participants With Short Bowel Syndrome</brief_title>
  <official_title>A Prospective, Multi-center Registry for Patients With Short Bowel Syndrome</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shire</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
