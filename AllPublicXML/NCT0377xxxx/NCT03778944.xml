  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <intervention_model_description>Using computer &amp; sealed envelope randomization, patients are assigned to receive either Mannitol [M group; n=20], Dopamine [D group; n=20] or adequate hydration [C group; n=20]</intervention_model_description>
    <primary_purpose>Supportive Care</primary_purpose>
    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Post-operative creatinine clearance</measure>
    <time_frame>7 days</time_frame>
    <description>Post-operative creatinine clearance level of the transplanted kidney graft is measured in ml/min</description>
  </primary_outcome>
  <number_of_arms>3</number_of_arms>
  <enrollment type="Anticipated">60</enrollment>
  <condition>Renal Failure Chronic</condition>
  <condition>Transplant;Failure,Kidney</condition>
  <condition>Anesthesia</condition>
  <arm_group>
    <arm_group_label>M group</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Mannitol infusion</description>
  </arm_group>
  <arm_group>
    <arm_group_label>D group</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Dopamine infusion</description>
  </arm_group>
  <arm_group>
    <arm_group_label>C group</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Adequate hydration</description>
  </arm_group>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>Mannitol infusion</intervention_name>
    <description>Infusion of Mannitol 20% at a dose of 0.5 mg/kg to the renal graft donor after induction of anesthesia over 15 minutes.</description>
    <arm_group_label>M group</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>Dopamine infusion</intervention_name>
    <description>Infusion of Dopamine at a dose of 4 microg/kg/min to the renal graft donor after induction of anesthesia till ligation of the renal artery.</description>
    <arm_group_label>D group</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Procedure</intervention_type>
    <intervention_name>Adequate hydration</intervention_name>
    <description>Infusion of Ringer Acetate at a rate of 15 ml/kg/hr to the renal graft donor after induction of anesthesia till ligation of the renal artery.</description>
    <arm_group_label>C group</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  end-stage renal disease, for living-donor kidney transplantation

        Exclusion Criteria:

          -  severe cardiac or hepatic dysfunction

          -  coagulopathy
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>60 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
