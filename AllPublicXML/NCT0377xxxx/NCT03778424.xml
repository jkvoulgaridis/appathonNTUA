    <expanded_access_type_treatment>Yes</expanded_access_type_treatment>
  </expanded_access_info>
  <condition>Lennox Gastaut Syndrome</condition>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Rufinamide</intervention_name>
    <description>Rufinamide up to 45 milligram per kilogram per day (mg/kg/day).</description>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Participants who were on rufinamide treatment and have completed Study E2080-G000-303
             in Poland.

        Exclusion Criteria:

          -  Participants were randomized to the other antiepileptic drug (AED) treatment group in
             study E2080-G000-303.
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>4 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
  </eligibility>
