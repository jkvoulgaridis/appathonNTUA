  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Validation of mVPAQ</measure>
    <time_frame>1 week</time_frame>
    <description>Cognitive debrief and usability through interviews and completion of an electronic diary</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Validation of mFSFI</measure>
    <time_frame>1 week</time_frame>
    <description>Cognitive debrief and usability through interviews and completion of an electronic diary</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Validation of Pain on Intercourse Numeric Rating Scale (NRS)</measure>
    <time_frame>1 week</time_frame>
    <description>Cognitive debrief and usability through interviews and completion of an electronic diary. Pain on Intercourse NRS that describes pain experienced during this sexual intercourse episode by number. The higher is the number the worse is pain.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Vulvar pain NRS</measure>
    <time_frame>1 week</time_frame>
    <description>Assess understanding of the appropriateness of the vulvar pain NRS through interview and completion of an electronic diary</description>
  </secondary_outcome>
  <enrollment type="Actual">20</enrollment>
  <condition>Vulvodynia</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Subjects referred by clinicians
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Fluent in understanding, speaking and reading US-English

          -  Have vulvodynia with pain for at least 6 months

        Exclusion Criteria:

          -  Clinically significant history of alcohol/drug abuse or dependence within the last 2
             years
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
