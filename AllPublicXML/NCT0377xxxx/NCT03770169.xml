  <brief_title>Validation of Patient Reported Outcome Measures for Use in Vulvodynia</brief_title>
  <official_title>Validation of Patient Reported Outcome Measures for Use in Vulvodynia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ipsen</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
