  <brief_title>Evaluation of Effectiveness and Safety of XIENCE Sierra in Routine Clinical Practice</brief_title>
  <acronym>IRIS Sierra</acronym>
  <official_title>Evaluation of Effectiveness and Safety of XIENCE Sierra in Routine Clinical Practice; A MULTICENTER, PROSPECTIVE OBSERVATIONAL STUDY</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seung-Jung Park</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>CardioVascular Research Foundation, Korea</agency>
      <agency_class>Other</agency_class>
    </collaborator>
