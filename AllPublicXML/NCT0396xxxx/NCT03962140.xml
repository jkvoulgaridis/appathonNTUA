  <brief_summary>
    <textblock>
      This study is a retrospective study to investigate the association between body fat
      distribution and sarcopenia on CT-scan and oncological outcomes in endometrial cancer
      patients
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">August 1, 2019</start_date>
  <completion_date type="Anticipated">March 1, 2021</completion_date>
  <primary_completion_date type="Anticipated">March 1, 2021</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Retrospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>The Investigation of Patients with High BMI and Survival Rates</measure>
    <time_frame>10 years</time_frame>
    <description>To investigate the association between body fat distribution and sarcopenia on CT-scan and survival in endometrial cancer patients</description>
  </primary_outcome>
  <enrollment type="Anticipated">350</enrollment>
  <condition>Endometrial Cancer</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Endometrial cancer patients
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Confirmed diagnosis of endometrial cancer

          -  Age: 18 years or older

          -  Abdominal CT-scan as part of pre-operative or post-operative staging work-up (&lt;1 month
             after surgery)

        Exclusion Criteria:

          -  Women with concurrent cancer

          -  Women with unknown body mass index
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>99 Years</maximum_age>
  </eligibility>
