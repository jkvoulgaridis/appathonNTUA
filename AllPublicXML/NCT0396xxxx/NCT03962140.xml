  <brief_title>CT Assessment of BMI in Endometrial Cancer Patients</brief_title>
  <official_title>Computed Tomography Assessment of Body Fat Distribution in Endometrial Cancer Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Royal Cornwall Hospitals Trust</agency>
