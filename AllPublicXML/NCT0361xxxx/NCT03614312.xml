  <brief_title>Placenta Development Study</brief_title>
  <official_title>Epigenetic Mechanisms Underlying Trophoblast Syncytialization</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Arkansas Children's Hospital Research Institute</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
