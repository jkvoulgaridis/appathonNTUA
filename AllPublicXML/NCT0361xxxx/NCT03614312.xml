  <primary_outcome>
    <measure>Number of participants that placental syncytial fusion involves epigenetic changes in histone modifications</measure>
    <time_frame>40 weeks</time_frame>
    <description>Number of participants that histone acetyltransferases eszymes, CBP and p300 affect syncytial fusion</description>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">25</enrollment>
  <condition>Healthy</condition>
  <arm_group>
    <arm_group_label>BMI 18-25 Pregnant Women</arm_group_label>
    <description>BMI 18-25 less than 36 weeks pregnant</description>
  </arm_group>
  <biospec_retention>Samples Without DNA</biospec_retention>
