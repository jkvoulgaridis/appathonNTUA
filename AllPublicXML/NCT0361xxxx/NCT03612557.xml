  <primary_outcome>
    <measure>Maximum observed plasma concentration for penicillin G (Cmax)</measure>
    <time_frame>0, 3, 6, 12, 24, 36, 48, 144, 312, 480, and 648 hours post dose</time_frame>
  </primary_outcome>
  <primary_outcome>
