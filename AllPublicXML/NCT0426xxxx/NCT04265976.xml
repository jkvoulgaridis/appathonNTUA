  </primary_outcome>
  <enrollment type="Anticipated">250</enrollment>
  <condition>Hypernatremia</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Every &gt;18 years old patient from who a blood sample was taken in the ED with a natremia &gt;
        145 mmol/l and don't show a refusal after information.
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Natremia &gt; 145 mmol/l

        Exclusion Criteria:

          -  Age &lt; 18 years

          -  Refusal
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
