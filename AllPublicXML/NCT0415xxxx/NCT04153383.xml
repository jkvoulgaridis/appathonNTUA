  <study_design_info>
    <observational_model>Case-Crossover</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>peak velocity of mitral annular tissue Doppler during early relaxation</measure>
    <time_frame>15 mini after milrinone administration</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>peak velocity of mitral annular tissue Doppler during atrial contraction</measure>
    <time_frame>15 mini after milrinone administration</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>peak velocity of mitral annular tissue Doppler during systole</measure>
    <time_frame>15 min after milline administration</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Global longitudinal strain of the left ventricle</measure>
    <time_frame>15 mini after milrinone administration</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">20</enrollment>
  <condition>Ischemic Heart Disease</condition>
  <arm_group>
