  <brief_title>Skin Incision Adhesive in Pediatrics</brief_title>
  <official_title>Evaluation of Surgiseal as a Topical Emergency Department Skin Incision Adhesive in Pediatrics A Case Series</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medline Industries</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
