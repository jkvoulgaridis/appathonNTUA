  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      This prospective multicenter registry study aims to detect the serial changes in
      ventriculo-ventricular conduction times according to cardiac-resynchronization therapy
      response.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">July 20, 2018</start_date>
  <completion_date type="Anticipated">July 20, 2024</completion_date>
