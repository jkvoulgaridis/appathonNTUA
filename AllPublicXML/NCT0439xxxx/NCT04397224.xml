  </study_design_info>
  <primary_outcome>
    <measure>Serial change of ventriculo-ventricular conduction times</measure>
    <time_frame>from enrollment to last follow-up (2 years)</time_frame>
    <description>Reduction or extension of ventriculo-ventricular conduction times</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">100</enrollment>
  <condition>Heart Failure</condition>
  <arm_group>
