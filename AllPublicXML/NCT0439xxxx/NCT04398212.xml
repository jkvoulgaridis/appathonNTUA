  <brief_title>Computer Vision Syndrome Visual Sequelae</brief_title>
  <official_title>Visual and Ocular Sequelae of Computer Vision Syndrome: A Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Sohag University</agency>
      <agency_class>Other</agency_class>
