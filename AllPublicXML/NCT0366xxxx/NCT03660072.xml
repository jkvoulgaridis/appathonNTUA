  <brief_title>Postmarketing Study of Empliciti in Korean Patients With Multiple Myeloma</brief_title>
  <official_title>Empliciti (Elotuzumab) Postmarketing Surveillance in Korean Patients With Multiple Myeloma</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bristol-Myers Squibb</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
