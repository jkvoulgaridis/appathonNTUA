  <primary_outcome>
    <measure>incidence of postoperative agitation</measure>
    <time_frame>5 minutes in the recovery room</time_frame>
    <description>0=none; 1=occur</description>
  </primary_outcome>
  <primary_outcome>
    <measure>incidence of postoperative agitation</measure>
    <time_frame>30 minutes in the recovery room</time_frame>
    <description>0=none; 1=occur</description>
  </primary_outcome>
  <primary_outcome>
    <measure>severity of postoperative agitation</measure>
    <time_frame>5 minutes in the recovery room</time_frame>
    <description>Four-point agitation scale (0=calm; 1=not calm but could easily become calmed; 3=not easily calmed, moderately agitated and restless; 4=combative, excited, or disoriented)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>severity of postoperative agitation</measure>
    <time_frame>30 minutes in the recovery room</time_frame>
    <description>Four-point agitation scale (0=calm; 1=not calm but could easily become calmed; 3=not easily calmed, moderately agitated and restless; 4=combative, excited, or disoriented)</description>
  </primary_outcome>
