    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Treatment</primary_purpose>
    <masking>Single (Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change in maximum keratometry</measure>
    <time_frame>6 months</time_frame>
    <description>assessed by corneal tomography</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in corrected distance visual acuity</measure>
    <time_frame>6 months</time_frame>
    <description>assessed with Snellen chart</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in uncorrected distance visual acuity</measure>
    <time_frame>6 months</time_frame>
    <description>assessed with Snellen chart</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
