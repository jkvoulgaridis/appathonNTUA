  <brief_title>Impact of Cardiac Complications on Outcomes of Patients After Subarachnoid Hemorrhage</brief_title>
  <official_title>Impact of Cardiac Complications on Outcomes of Patients After Subarachnoid Hemorrhage</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Assiut University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
