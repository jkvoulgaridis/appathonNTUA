  <brief_title>Cord Blood Microtransplantation for Treatment of Acute Myeloid Leukemia</brief_title>
  <official_title>Cord Blood Microtransplantation for Treatment of Acute Myeloid Leukemia</official_title>
  <sponsors>
    <lead_sponsor>
