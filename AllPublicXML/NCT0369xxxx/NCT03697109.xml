  <brief_title>A Study of the Efficacy and Safety of Relacorilant in Patients With Endogenous Cushing Syndrome</brief_title>
  <acronym>GRACE</acronym>
  <official_title>Glucocorticoid Receptor Antagonism in the Treatment of Cushing Syndrome (GRACE): A Phase 3, Double-Blind, Placebo-Controlled, Randomized-Withdrawal Study of the Efficacy and Safety of Relacorilant</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Corcept Therapeutics</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Corcept Therapeutics</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
