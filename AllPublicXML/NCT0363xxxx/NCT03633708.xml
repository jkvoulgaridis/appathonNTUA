    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Change of intact parathyroid hormone (iPTH) levels in children ages 28d to &lt;18y with secondary hyperparathyroidism (sHPT) receiving maintenance hemodialysis</measure>
    <time_frame>32 weeks</time_frame>
    <description>Achievement of at least a 30% reduction from baseline in mean iPTH during the efficacy assessment phase (EAP)</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Corrected serum calcium levels of &lt; 8.0 mg/dL in children aged 2 years to &lt;18 years old</measure>
    <time_frame>32 weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Corrected serum calcium levels of &lt; 8.6 mg/dL in children aged 28 days to &lt;18 years old</measure>
    <time_frame>32 weeks</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">56</enrollment>
  <condition>Secondary Hyperparathyroidism</condition>
  <condition>Chronic Kidney Disease</condition>
