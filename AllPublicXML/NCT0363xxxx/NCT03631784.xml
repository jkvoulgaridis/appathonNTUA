    <arm_group_label>Cohort B</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Pemetrexed 500 mg/m^2</intervention_name>
    <description>Pemetrexed 500 mg/m^2 IV infusion on Day 1 of each 21-day cycle for Cycles 1, 2, and 3.</description>
