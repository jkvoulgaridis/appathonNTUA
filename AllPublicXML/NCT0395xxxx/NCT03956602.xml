  <brief_title>Effects of Snack Types on Acute Glucose and Insulin Response</brief_title>
  <official_title>Effects of Snack Types on Acute Glucose and Insulin Response</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>San Diego State University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
