  <brief_title>Effects of Fresh Mango Pulp Consumption on Satiety and Cardiometabolic Health</brief_title>
  <official_title>Effects of Fresh Mango Pulp Consumption on Satiety and Cardiometabolic Health</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>San Diego State University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
