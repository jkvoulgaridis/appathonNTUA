  <brief_title>Evaluation of Cesarean Scar After Three Different Uterine Closure Technis</brief_title>
  <official_title>Evaluation of Cesarean Scar After Three Different Uterine Closure Technis: A Randomised Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ataturk University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
