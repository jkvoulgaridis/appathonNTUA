  <brief_title>Platelet Rich Fibrin(PRF) Injection for Treatment of Dental Pulp Exposure:Randomized Clinical Trial</brief_title>
  <official_title>Platelet Rich Fibrin(PRF) Versus Pulp Capping in Treatment of Dental Pulp Exposure:Randomized Clinical Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Cairo University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Cairo University</source>
