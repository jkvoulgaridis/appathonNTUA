  </study_design_info>
  <primary_outcome>
    <measure>Corneal hysteresis</measure>
    <time_frame>Baseline</time_frame>
    <description>Difference in postoperative corneal hysteresis (measured in mm Hg) by Ocular Response Analyzer, between study arms</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Incidence of anterior corneal striae</measure>
    <time_frame>Baseline</time_frame>
    <description>Frequency of occurrence among all enrolled hypotonous eyes</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Incidence of hypotony maculopathy</measure>
    <time_frame>Baseline</time_frame>
    <description>Frequency of occurrence among all enrolled hypotonous eyes</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Axial length</measure>
