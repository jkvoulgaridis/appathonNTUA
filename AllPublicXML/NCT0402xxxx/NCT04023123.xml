  </secondary_outcome>
  <secondary_outcome>
    <measure>Corneal resistance factor</measure>
    <time_frame>Baseline</time_frame>
    <description>Difference in postoperative corneal resistance factor between study arms</description>
  </secondary_outcome>
  <number_of_groups>3</number_of_groups>
  <enrollment type="Actual">98</enrollment>
