  <brief_title>Anterior Corneal Striae in Hypotony</brief_title>
  <official_title>The Clinical Significance of Anterior Corneal Striae in Hypotony</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Duke University</agency>
      <agency_class>Other</agency_class>
