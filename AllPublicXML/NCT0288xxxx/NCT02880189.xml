  <brief_title>EUS Guided Core Liver Biopsy and IGB Placement for the Diagnosis and Management of NASH and Obesity</brief_title>
  <official_title>Combined Endoscopic Ultrasound Guided Core Liver Biopsy and Intragastric Balloon Placement for the Diagnosis and Management of Nonalcoholic Steatohepatitis and Obesity</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
