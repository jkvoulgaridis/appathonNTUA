  <brief_title>Virtual Behavioral Health Integration</brief_title>
  <acronym>VBHI</acronym>
  <official_title>An Evaluation of Virtual Behavioral Health Integration Into Primary Care</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Atrium Health</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
