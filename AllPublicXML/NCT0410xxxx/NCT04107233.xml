  <brief_title>Using Electronic Medical Record Data to Improve HIV Primary Care</brief_title>
  <official_title>Using Electronic Medical Record Data to Improve HIV Primary Care: An Audit and Feedback Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Michael's Hospital, Toronto</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
