  <brief_title>Trichomonas Vaginalis Repeat Infections Among HIV Negative Women</brief_title>
  <official_title>Trichomonas Vaginalis Repeat Infections Among HIV Negative Women</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tulane University Health Sciences Center</agency>
