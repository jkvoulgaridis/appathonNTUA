  <primary_outcome>
    <measure>Hypoxemia</measure>
    <time_frame>15 days</time_frame>
    <description>PO2 under 60mmHg, or SatO2 under 80%; that leads to the instauration of invasive ventilation</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Tachypnea</measure>
    <time_frame>15 days</time_frame>
    <description>Respiratory rate over 20; that leads to the instauration of invasive ventilation</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Hypercapnia</measure>
    <time_frame>15 days</time_frame>
    <description>PCO2 over 55mmHg; that leads to the instauration of invasive ventilation</description>
  </primary_outcome>
