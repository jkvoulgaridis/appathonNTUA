  <brief_title>A Study of LY3002813 in Participants With Early Symptomatic Alzheimer's Disease (TRAILBLAZER-ALZ)</brief_title>
  <official_title>Assessment of Safety, Tolerability and Efficacy of LY3002813 in Early Symptomatic Alzheimer's Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Eli Lilly and Company</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
