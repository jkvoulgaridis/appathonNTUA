  <brief_title>CYP2B6 Polymorphisms in Methadone Disposition</brief_title>
  <official_title>Role of CYP2B6 Polymorphisms in Methadone Metabolism and Clearance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Washington University School of Medicine</agency>
      <agency_class>Other</agency_class>
