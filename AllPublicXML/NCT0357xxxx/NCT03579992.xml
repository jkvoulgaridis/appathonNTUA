  </arm_group>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>60-min isometric MyCI training</intervention_name>
    <description>EMG-controlled in-lab training with arm restrained for 60 minutes per session</description>
    <arm_group_label>60-minute Isometric</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Behavioral</intervention_type>
    <intervention_name>90-min isometric MyCI training</intervention_name>
    <description>EMG-controlled in-lab training with arm restrained for 90 minutes per session</description>
