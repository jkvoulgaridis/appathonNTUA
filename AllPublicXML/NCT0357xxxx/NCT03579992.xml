    <arm_group_label>60-minute Isometric</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>60-min Isometric MyCI training: EMG-controlled game training for 60-minutes per session</description>
  </arm_group>
  <arm_group>
    <arm_group_label>90-minute Isometric</arm_group_label>
