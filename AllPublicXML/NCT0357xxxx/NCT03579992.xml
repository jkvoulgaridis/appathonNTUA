  <brief_title>Myoelectric Computer Interface to Reduce Muscle Co-activation After Stroke</brief_title>
  <official_title>Myoelectric Computer Interface to Reduce Muscle Co-activation After Stroke</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
