  <target_duration>2 Years</target_duration>
  <primary_outcome>
    <measure>Composite of Major Adverse Events (MAEs)</measure>
    <time_frame>30 days</time_frame>
    <description>All-cause mortality, major bleeding, symptomatic PE, or rethrombosis of target venous segment</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Technical Success</measure>
    <time_frame>Index procedure</time_frame>
    <description>Complete or near complete (75% or greater) removal of venous thrombus from the target venous segment</description>
  </secondary_outcome>
