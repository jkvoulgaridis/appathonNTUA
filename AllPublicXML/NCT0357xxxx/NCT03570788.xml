  <brief_title>HRQoL Among Patients Amputated Due to Peripheral Arterial Disease</brief_title>
  <official_title>Health-related Quality of Life Among Patients Amputated Due to Peripheral Arterial Disease</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Jonas Malmstedt</agency>
      <agency_class>Other</agency_class>
