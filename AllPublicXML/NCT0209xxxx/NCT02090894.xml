  <brief_title>Effect of Dermal Rejuvenation on the UVB Response of Geriatric Skin</brief_title>
  <acronym>Laser Genesis</acronym>
  <official_title>Effect of Dermal Rejuvenation on the UVB Response of Geriatric Skin</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Indiana University</agency>
      <agency_class>Other</agency_class>
