    <primary_purpose>Treatment</primary_purpose>
    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Pain scores</measure>
    <time_frame>1 hr post-op</time_frame>
    <description>Pain scores &gt; 5 (out of 10) in the post anesthesia care unit (PACU).</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Extubation time</measure>
    <time_frame>1 hr post-op</time_frame>
    <description>Amount of time in the PACU before patient is ready to be extubated.</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Time in PACU</measure>
    <time_frame>1-2 hr post-op</time_frame>
    <description>Total time in PACU before patient met discharge criteria.</description>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">10</enrollment>
  <condition>Adenoid Hypertrophy</condition>
  <arm_group>
    <arm_group_label>Non-opiod pain relief</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Subjects will receive preoperative oral dextromethorphan &amp; acetaminophen and intraoperative intravenous dexmedetomidine &amp; ketamine.</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Dextromethorphan</intervention_name>
    <description>Preoperative oral dextromethorphan 1 mg/kg</description>
    <arm_group_label>Non-opiod pain relief</arm_group_label>
    <other_name>Robitussin</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Acetaminophen</intervention_name>
    <description>Preoperative oral acetaminophen 15 mg/kg</description>
    <arm_group_label>Non-opiod pain relief</arm_group_label>
    <other_name>Tylenol</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Dexmedetomidine</intervention_name>
    <description>Intraoperative intravenous dexmedetomidine 0.5 μg/kg</description>
    <arm_group_label>Non-opiod pain relief</arm_group_label>
    <other_name>Precedex</other_name>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Ketamine</intervention_name>
    <description>Intraoperative intravenous ketamine 0.5 mg/kg</description>
    <arm_group_label>Non-opiod pain relief</arm_group_label>
    <other_name>Ketalar</other_name>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          1. Patients undergoing adenoidectomy ± bilateral ear tube placement

          2. Have a parent/guardian who are compliant with routine medical care, capable of
             subjective evaluation and able to read, understand and sign the informed consent in
             accordance with IRB regulations

        Exclusion Criteria:

          1. An American Society of Anesthesiologists Physical Status ≥ 4 (severe disease that is
             life threatening);

          2. A known hypersensitivity or allergy to any of the study medications;

          3. A history of chronic opioid use prior to surgery;

          4. Coexisting renal or hepatic disease;

          5. Morbid obesity (BMI% ≥ 99).
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>4 Years</minimum_age>
    <maximum_age>8 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
