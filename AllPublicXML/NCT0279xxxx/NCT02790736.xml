  <brief_title>Brief Intervention to Reduce Fear of Public Speaking</brief_title>
  <official_title>Brief Intervention to Reduce Fear of Public Speaking</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>New York State Psychiatric Institute</agency>
