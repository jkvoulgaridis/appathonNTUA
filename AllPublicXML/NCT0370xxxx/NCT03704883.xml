  <brief_title>Behavior of Cyclists in Response to Pain</brief_title>
  <official_title>Behavior of Cyclists in Response to Pain: a Qualitative Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allina Health System</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
