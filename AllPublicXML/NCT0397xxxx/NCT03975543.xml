  <brief_title>Retrospective Natural History Study of Retinitis Pigmentosa</brief_title>
  <acronym>PHENOROD1</acronym>
  <official_title>Natural History Study of Retinitis Pigmentosa in Patient Carrying Pathogenic Mutations in RHO, PDE6A or PDE6B.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>SparingVision</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
