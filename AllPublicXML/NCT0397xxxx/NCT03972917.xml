  <eligibility>
    <study_pop>
      <textblock>
        Italian fertile women who are eligible for conservative treatment for uterine fibroids
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  number of myomas from 1 to 4

          -  heavy menstrual bleeding

          -  conservative treatment request

        Exclusion Criteria:

          -  concurrent endometrial pathology

          -  post-menopausal status

          -  hepatic pathology

          -  endocrine or metabolic disorders
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>45 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
