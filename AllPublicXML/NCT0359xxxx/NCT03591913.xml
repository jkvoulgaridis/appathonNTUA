  <primary_outcome>
    <measure>mean intraoperative blood loss</measure>
    <time_frame>24 hours</time_frame>
    <description>Blood loss estimation will commence immediately following the skin incision</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
