  <arm_group>
    <arm_group_label>study group</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>will receive sublingual misoprostol immediately after urinary catheterization and before skin incision</description>
  </arm_group>
  <arm_group>
    <arm_group_label>control group</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
