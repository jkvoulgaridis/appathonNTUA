      <agency>Pfizer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Pfizer</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
