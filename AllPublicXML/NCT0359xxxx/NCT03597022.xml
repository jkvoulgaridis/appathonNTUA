  <brief_title>Multiple Escalating Dose Study of BAY1093884 in Adults With Hemophilia A or B With or Without Inhibitors</brief_title>
  <official_title>Multiple Escalating Dose Study of BAY1093884 in Adults With Hemophilia A or B With or Without Inhibitors</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bayer</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Bayer</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
    <is_fda_regulated_drug>Yes</is_fda_regulated_drug>
