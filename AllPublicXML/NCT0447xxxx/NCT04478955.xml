  <brief_title>Effect of Eye Make up on Ocular Surface</brief_title>
  <official_title>Effect of Pencil Eyeliners and Waterproof Mascara on Tear Stability Tests and Meibomography</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Erol Olcok Corum Training and Research Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
