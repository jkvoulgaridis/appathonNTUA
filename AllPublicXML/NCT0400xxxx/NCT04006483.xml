  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Positive control dentifrice</intervention_name>
    <description>Positive control dentifrice containing (0.3%) triclosan.</description>
    <arm_group_label>Positive control dentifrice</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Negative control dentifrice</intervention_name>
    <description>Negative control dentifrice containing either sodium fluoride (0.243%) or sodium monofluorophosphate (0.76%)</description>
    <arm_group_label>Negative control dentifrice</arm_group_label>
  </intervention>
