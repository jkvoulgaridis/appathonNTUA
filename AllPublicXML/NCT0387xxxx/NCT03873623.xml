  <brief_title>The TOGETHER Project - Kidney</brief_title>
  <acronym>TOGETHER</acronym>
  <official_title>The TOGETHER Project - Transplant Organ Genomics to Help Prevent Rejection in Kidney Transplant Recipients With Realtime Assay Monitoring</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mayo Clinic</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
