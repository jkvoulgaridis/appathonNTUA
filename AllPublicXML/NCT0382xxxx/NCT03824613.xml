  <brief_title>Urinary miRNA in Endometrial Cancer Study</brief_title>
  <official_title>Urinary microRNA Expression in Endometrial Cancer Patients - a Feasibility Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Royal Cornwall Hospitals Trust</agency>
      <agency_class>Other</agency_class>
