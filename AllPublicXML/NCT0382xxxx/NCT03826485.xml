  <primary_outcome>
    <measure>Area Under the Curve (AUC) to last time point (t)</measure>
    <time_frame>0, 0.5, 0.75, 1, 1.5, 1.75, 2, 2.5, 3, 4, 6, 8, 10 and 12hours postdose</time_frame>
    <description>Area under the PRIC/Pranlukast dry syrup concentration in blood-time curve from zero to the final</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Maximum concentration of drug (Cmax)</measure>
    <time_frame>0, 0.5, 0.75, 1, 1.5, 1.75, 2, 2.5, 3, 4, 6, 8, 10 and 12hours postdose</time_frame>
    <description>The maximum PRIC/Pranlukast dry syrup concentration in blood sampling time t</description>
  </primary_outcome>
