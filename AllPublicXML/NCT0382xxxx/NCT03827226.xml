  <brief_title>Breech External Cephalic Version Intervention Trial</brief_title>
  <acronym>BREXIT</acronym>
  <official_title>Breech External Cephalic Version Intervention Trial- A Prospective Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Charite University, Berlin, Germany</agency>
      <agency_class>Other</agency_class>
