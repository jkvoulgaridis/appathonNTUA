  </study_design_info>
  <primary_outcome>
    <measure>proportion of subjects with an anterior chamber cell grade of 0 at Day 15</measure>
    <time_frame>Day 15</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>the proportion of subjects who achieve a pain score of 0 at Day 15</measure>
    <time_frame>Day 15</time_frame>
  </secondary_outcome>
