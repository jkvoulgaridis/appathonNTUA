  <primary_outcome>
    <measure>ARID5B polymorphism/mutation status</measure>
    <time_frame>5 years</time_frame>
    <description>Correlation of our experimentally validated Arid5b dependent transcriptional signature with primary patient samples measured by patient ARID5B expression and polymorphisms.</description>
  </primary_outcome>
  <enrollment type="Anticipated">50</enrollment>
