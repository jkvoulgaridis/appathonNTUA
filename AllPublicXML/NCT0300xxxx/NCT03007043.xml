  </study_design_info>
  <primary_outcome>
    <measure>prevalence of the main FSH receptor polymorphism</measure>
    <time_frame>12 months</time_frame>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Actual">364</enrollment>
  <condition>Fertility Disorders</condition>
  <arm_group>
    <arm_group_label>Normal responders</arm_group_label>
    <description>Normal response following IVF</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Suboptimal responders</arm_group_label>
