  <brief_summary>
    <textblock>
      This is an observational study using optical coherence tomography (OCT) technology to
      quantify corneal edema in Fuchs' endothelial dystrophy and predict refractive shift from
      resolving corneal edema after surgical treatments.
    </textblock>
  </brief_summary>
  <overall_status>Not yet recruiting</overall_status>
  <start_date type="Anticipated">June 2020</start_date>
  <completion_date type="Anticipated">June 2023</completion_date>
  <primary_completion_date type="Anticipated">June 2023</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
