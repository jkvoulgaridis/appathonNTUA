    <study_pop>
      <textblock>
        Participants admitted to the ICU with a primary diagnosis of acute heart failure,
        respiratory failure, cardiac arrest, aged 18 years or older, and receive ECMO at the
        participating ECMO centres.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients admitted to ICU and receive ECMO in Australia and New Zealand

          -  Patients aged 18 years or older

        Nil Exclusion Criteria:
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
