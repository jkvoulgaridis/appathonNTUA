  <primary_outcome>
    <measure>AF and SR classification accuracy</measure>
    <time_frame>1 visit (30 minutes)</time_frame>
    <description>sensitivity and specificity of detection of AF and SR from 1-lead ECG against reference 12-lead ECG</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Quality of the 1-lead ECG strips</measure>
    <time_frame>1 visit (30 minutes)</time_frame>
    <description>sensitivity and specificity of detection of AF and SR from 1-lead ECG strips reviewed by a blind, trained cardiologist against reference 12-lead ECG</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>safety evaluation</measure>
    <time_frame>1 visit (30 minutes)</time_frame>
    <description>frequency of adverse events</description>
  </secondary_outcome>
