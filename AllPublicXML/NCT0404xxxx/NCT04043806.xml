    <masking>None (Open Label)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Safety and tolerability as assessed by number of subjects with adverse events (AEs) and serious adverse events (SAEs)</measure>
    <time_frame>From Baseline through Week 100</time_frame>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">480</enrollment>
  <condition>Cystic Fibrosis</condition>
