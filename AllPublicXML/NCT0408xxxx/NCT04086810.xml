  <primary_outcome>
    <measure>Adverse Events (Safety)</measure>
    <time_frame>Baseline to Week 106</time_frame>
    <description>Assess the safety of DCCR by evaluating the incidence and severity of adverse events reported.</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
