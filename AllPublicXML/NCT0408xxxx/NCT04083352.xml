  <brief_title>Ketone Supplementation in Individuals With PTSD</brief_title>
  <official_title>The Effects of Exogenous Ketone Supplementation or a Ketogenic Diet in Individuals With PTSD</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Augusta University</agency>
      <agency_class>Other</agency_class>
