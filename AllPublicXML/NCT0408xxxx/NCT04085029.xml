  <brief_summary>
    <textblock>
      The purpose of this study is to provide a registry of participants in order to assess the
      acute adverse event rates following ablative radiotherapy for metastatic disease.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">August 28, 2019</start_date>
  <completion_date type="Anticipated">September 2029</completion_date>
  <primary_completion_date type="Anticipated">September 2029</primary_completion_date>
  <study_type>Observational [Patient Registry]</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Only</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>10 Years</target_duration>
  <primary_outcome>
    <measure>Occurrence of acute adverse events following radiotherapy</measure>
    <time_frame>90 Days</time_frame>
  </primary_outcome>
  <enrollment type="Anticipated">1000</enrollment>
  <condition>Oligometastasis</condition>
  <condition>Metastatic Cancer</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Patients at any Duke University Health System Radiation Oncology treatment facility
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age ≥ 18 years of age

          -  Radiographic evidence of metastatic disease (may include oligoprogression,
             oligorecurrence or oligometastasis) at the time of study enrollment

          -  Planned to receive, currently receiving, or have completed hypofractionated image
             guided radiotherapy (ablative radiotherapy) to an extracranial site(s)

          -  Patient must sign study-specific informed consent

        Exclusion Criteria:

        • None
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
