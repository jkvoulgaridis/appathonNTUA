  <brief_title>Effects of Motor Imagery in Pain Modulation and Median Nerve Mechanosensitive in Healthy Patients</brief_title>
  <official_title>Effects of Motor Imagery in Pain Modulation and Median Nerve Mechanosensitive in Healthy Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universidad Rey Juan Carlos</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
