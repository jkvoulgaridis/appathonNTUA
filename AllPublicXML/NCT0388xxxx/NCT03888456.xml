  <primary_outcome>
    <measure>temperature</measure>
    <time_frame>1 week</time_frame>
    <description>variability of the autonomic nervous system measured by temperature variability of the face</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>skin conductance</measure>
    <time_frame>1 week</time_frame>
    <description>pre-post changes of physiological parameter of electrodermal skin response</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>heart rate variability</measure>
    <time_frame>1 week</time_frame>
    <description>pre-post changes of HRV</description>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
