  <brief_title>Nutrient Nutritional Advise for Low Back Pain Patients Seeking Chiropractic Care</brief_title>
  <official_title>Nutrition Plus Chiropractic Care for Low Back Pain: a Randomized Clinical Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Parker University</agency>
      <agency_class>Other</agency_class>
