    "axios": "^0.21.1",
    "document": "^0.4.7",
    "dom": "0.0.3",
    "dom-parser": "^0.1.6",
    "he": "^1.2.0",
    "jsdom": "^16.3.0",
    "tslib": "^1.13.0",
    "xml": "^1.0.1",
    "xml-parse": "^0.4.0",
    "xml-parse-from-string": "^1.0.1",
    "xmldom": "^0.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
